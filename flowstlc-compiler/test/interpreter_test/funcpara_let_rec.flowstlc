val ten : Int = 10
val flag : Bool = true

fun fact : Int^Pub -> Int
fun fact n =
    if n <= 1 then 1
    else n * (fact (n - 1))

fun inc : Int^Pub -> Int
fun inc x = x + 1

fun applyTwice : Int^Pub -> Int^Pub -> Int
fun applyTwice f x = f (f x)

fun main : Unit^Pub -> Int
fun main =
    let [a] = (fact 5) in
    (let [b] = (applyTwice inc (-ten)) in
    (let [c] = (if flag and b == 12 then a else 0) in
    c + b))